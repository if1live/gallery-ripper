{% extends "base.jinja2" %}
{% block body %}
{% if message %}
<div class="alert alert-danger" role="alert">
  {{ message }}
</div>
{% endif %}

<form role="form" action="/" method="POST">
  <div class="form-group">
	<input type="url" name="url" class="form-control"/>
  </div>
  <button type="submit" class="btn btn-default">GOTO</button>
</form>
{% endblock %}

